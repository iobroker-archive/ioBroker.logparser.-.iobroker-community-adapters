		"_parserRules": {
			"type": "panel",
			"label": "Parser-Rules (filter)",
			"items": {
				"headerParserRules": {
					"type": "header",
					"text": "Parser-Rules (filter)",
					"size": 2
				},
				"parserRules": {
					"type": "table",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"items": [
						{
							"type": "checkbox",
							"title": "",
							"attr": "active",
							"width": "5%"
						},
						{
							"type": "text",
							"title": "Name",
							"attr": "name",
							"width": "15%"
						},
						{
							"type": "text",
							"title": "Whitelist AND",
							"attr": "whitelistAnd",
							"width": "10%"
						},
						{
							"type": "text",
							"title": "Whitelist OR",
