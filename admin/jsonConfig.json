{
	"i18n": true,
	"type": "tabs",
	"items": {
		"_start": {
			"type": "panel",
			"label": "Start",
			"items": {}
		},
		"_parserRules": {
			"type": "panel",
			"label": "Parser-Rules (filter)",
			"items": {
				"headerParserRules": {
					"type": "header",
					"text": "Parser-Rules (filter)",
					"size": 2
				},
				"parserRules": {
					"type": "table",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"items": [
						{
							"type": "checkbox",
							"title": "Active",
							"attr": "active",
							"width": "5%"
						},
						{
							"type": "text",
							"title": "Name",
							"attr": "name",
							"width": "15%"
						},
						{
							"type": "text",
							"title": "Whitelist AND",
							"attr": "whitelistAnd",
							"width": "10%"
						},
						{
							"type": "text",
							"title": "Whitelist OR",
							"attr": "whitelistOr",
							"width": "10%"
						},
						{
							"type": "text",
							"title": "Blacklist",
							"attr": "blacklist",
							"width": "10%"
						},
						{
							"type": "checkbox",
							"title": "Debug",
							"attr": "debug",
							"width": "5%"
						},
						{
							"type": "checkbox",
							"title": "Info",
							"attr": "info",
							"width": "5%"
						},
						{
							"type": "checkbox",
							"title": "Warn",
							"attr": "warn",
							"width": "5%"
						},
						{
							"type": "checkbox",
							"title": "Error",
							"attr": "error",
							"width": "5%"
						},
						{
							"type": "number",
							"title": "Max",
							"attr": "maxLength",
							"width": "7%"
						},
						{
							"type": "text",
							"title": "Clean",
							"attr": "clean",
							"width": "10%"
						},
						{
							"type": "checkbox",
							"title": "Merge",
							"attr": "merge",
							"width": "5%"
						},
						{
							"type": "text",
							"title": "Dateformat",
							"attr": "dateformat",
							"width": "10%"
						}
					]
				}
			}
		},
		"_otherSettings": {
			"type": "panel",
			"label": "Other settings",
			"items": {
				"_headerRemovePid": {
					"type": "header",
					"text": "Remove PID",
					"size": 2
				},
				"_textRemovePid": {
					"type": "staticText",
					"text": "The js controller as of >= version 2.0 partly adds the PID in brackets to the front of logs, e.g. '(12345) Terminated: Without reason'. Enabling this option removes the PIDs including brackets, e.g. '(12345)', from the log lines.",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"style": {
						"fontSize": 16,
						"marginTop": 0
					}
				},
				"removePid": {
					"newLine": true,
					"type": "checkbox",
					"label": "Remove PID"
				},
				"_headerRemoveScriptName": {
					"type": "header",
					"text": "Remove script.js.Script_Name:",
					"size": 2
				},
				"_textRemoveScriptName": {
					"type": "staticText",
					"text": "Logs generated by the JavaScript adapter start with 'script.js.Script_Name:'. If this option is enabled, they are automatically removed.",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"style": {
						"fontSize": 16,
						"marginTop": 0
					}
				},
				"removeScriptJs": {
					"newLine": true,
					"type": "checkbox",
					"label": "Remove script.js.Script_Name"
				},
				"_headerReplaceDate": {
					"type": "header",
					"text": "Replace date with 'Today' / 'Yesterday':",
					"size": 2
				},
				"_textReplaceDate": {
					"type": "staticText",
					"text": "In the filters, today's or yesterday's date can be replaced by 'Today' or 'Yesterday' in the date format for using hash character (#). Other terms can be defined here instead of 'Today'/'Yesterday'.",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"style": {
						"fontSize": 16,
						"marginTop": 0
					}
				},
				"txtYesterday": {
					"newLine": true,
					"sm": 12,
					"md": 6,
					"lg": 6,
					"type": "text",
					"label": "Output for 'Yesterday'"
				},
				"txtToday": {
					"sm": 12,
					"md": 6,
					"lg": 6,
					"type": "text",
					"label": "Output for 'Today'"
				},
				"_headerMerge": {
					"type": "header",
					"text": "Text for 'Merge' (combine logs):",
					"size": 2
				},
				"_textMerge": {
					"type": "staticText",
					"text": "This text is prepended to each log line when merge is enabled. The # character is then replaced by the number of logs with the same content. Special characters like [](){} are allowed.",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"style": {
						"fontSize": 16,
						"marginTop": 0
					}
				},
				"_textMergeTwo": {
					"type": "staticText",
					"text": "Examples (without quotes): '[# entries]', '(#) ', '# entries: '",
					"sm": 12,
					"md": 12,
					"lg": 12,
					"style": {
						"fontSize": 16,
						"marginTop": 0
					}
				},
				"txtMerge": {
					"newLine": true,
					"sm": 12,
					"md": 6,
					"lg": 6,
					"type": "text",
					"label": "Prefixed text (# is replaced by number)"
				}
			}
		},
		"_visualization": {
			"type": "panel",
			"label": "Visualization",
			"items": {}
		},
		"_globalBlacklist": {
			"type": "panel",
			"label": "Global blacklist",
			"items": {}
		},
		"_expertSettings": {
			"type": "panel",
			"label": "Expert settings",
			"items": {}
		}
	}
}
